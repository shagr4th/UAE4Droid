            </intent-filter>
        </activity>
    <activity android:name="Settings" android:screenOrientation="landscape" android:configChanges="keyboardHidden|orientation"></activity>


<activity android:name="org.ab.nativelayer.ImportView" android:label="Choose a file" android:configChanges="keyboardHidden|orientation" android:screenOrientation="landscape"></activity>
