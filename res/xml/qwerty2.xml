<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >
    
    <Row>
        <Key android:codes="1059" android:keyLabel="LEFT SHIFT" android:keyWidth="30%p" android:keyEdgeFlags="left"/>
        <Key android:codes="1019" android:keyIcon="@drawable/arrow_up"/>
        <Key android:codes="1020" android:keyIcon="@drawable/arrow_down"/>
        <Key android:codes="1021" android:keyIcon="@drawable/arrow_left"/>
        <Key android:codes="1022" android:keyIcon="@drawable/arrow_right"/>
        <Key android:codes="1060" android:keyLabel="RIGHT SHIFT" android:keyWidth="30%p" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="-2" android:keyIcon="@drawable/sym_keyboard_shift" android:isModifier="true" android:keyEdgeFlags="left"/>
        <Key android:codes="1017" android:keyLabel="*"/>
        <Key android:codes="1069" android:keyLabel="-"/>
        <Key android:codes="1081" android:keyLabel="+"/>
        <Key android:codes="1055" android:keyLabel=","/>
        <Key android:codes="1056" android:keyLabel="."/>
        <Key android:codes="1074" android:keyLabel=";"/>
   		<Key android:codes="1076" android:keyLabel="/"/>
        <Key android:codes="1066" android:keyLabel="ENT" android:keyWidth="20%p" android:keyEdgeFlags="right"/>
    </Row>
     
  <Row>
        <Key android:codes="1092" android:keyLabel="Esc" android:keyWidth="20%p" android:keyEdgeFlags="left"/>
        <Key android:codes="1094" android:keyLabel="F2"/>
        <Key android:codes="1096" android:keyLabel="F4"/>
        <Key android:codes="1098" android:keyLabel="F6"/>
        <Key android:codes="1100" android:keyLabel="F8"/>
        <Key android:codes="1062" android:keyIcon="@drawable/sym_keyboard_space" android:keyWidth="40%p" android:keyEdgeFlags="right"/>
    </Row>
   

</Keyboard>
    